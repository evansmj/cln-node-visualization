{
  "name": "create-coreln-app",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://evansmj.github.io/cln-node-visualization/",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "preinstall": "mkdir -p build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "prepare": "smui-theme compile build/smui.css -i src/theme"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@smui/banner": "^7.0.0-beta.17",
    "@smui/button": "^7.0.0-beta.17",
    "@smui/checkbox": "^7.0.0-beta.17",
    "@smui/form-field": "^7.0.0-beta.17",
    "@smui/textfield": "^7.0.0-beta.17",
    "@smui/top-app-bar": "^7.0.0-beta.17",
    "@sveltejs/adapter-vercel": "^5.1.1",
    "@sveltejs/kit": "^2.5.4",
    "@types/d3": "^7.4.3",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "smui-theme": "^7.0.0-beta.17",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.7",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vitest": "^1.3.1"
  },
  "type": "module",
  "dependencies": {
    "d3": "^7.9.0",
    "graphology": "^0.25.4",
    "jest": "^29.7.0",
    "lnmessage": "^0.2.7",
    "rxjs": "^7.8.1",
    "sinon": "^17.0.1"
  }
}
