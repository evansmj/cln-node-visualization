{
  "name": "create-coreln-app",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://evansmj.github.io/cln-node-visualization/",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "preinstall": "mkdir -p build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "prepare": "smui-theme compile build/smui.css -i src/theme"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@smui/banner": "^7.0.0-beta.15",
    "@smui/button": "^7.0.0-beta.15",
    "@smui/checkbox": "^7.0.0-beta.15",
    "@smui/form-field": "^7.0.0-beta.15",
    "@smui/textfield": "^7.0.0-beta.15",
    "@smui/top-app-bar": "^7.0.0-beta.15",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^6.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "smui-theme": "^7.0.0-beta.15",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tailwindcss": "^3.3.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.2.0",
    "vitest": "^0.25.3"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-static": "^2.0.3",
    "d3": "^7.8.5",
    "graphology": "^0.25.4",
    "jest": "^29.7.0",
    "lnmessage": "^0.2.6",
    "rxjs": "^7.8.1",
    "sinon": "^17.0.1",
    "svelte-adapter-github": "^1.0.0-next.0"
  }
}
